package entities;

public class Aluno {
	public String name;
	public double n1;
	public double n2;
	public double n3;
	
	public Aluno(String name, double n1, double n2, double n3) {
		this.name = name;
		this.n1 = n1;
		this.n2 = n2;
		this.n3 = n3;
				
	}
	
	public double calculo(double n1, double n2, double n3) {
		return n1 + n2 + n3;
	}
	
	
	
}
